<?xml version="1.0"?>

<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
	  xmlns="http://www.w3.org/1999/xhtml"
	  version="1.2">
  <jsp:directive.page language="java" session="false" contentType="text/html" />
  
  <a name="keylist"><h1>Hot keys</h1></a>
  <dl class="keyslist">
    <dt>C-f, &lt;TAB&gt;</dt><dd>forward to the next unprotected
    field</dd>
    
    <dt><kbd>M-f</kbd>, <kbd>shift-TAB</kbd></dt><dd>backward to the previous
    unprotected field</dd>
    
    <dt>C-t, M-t</dt><dd>moves to the component next or before the
    terminal screen (focus jumps out of terminal screen)</dd>
  </dl>
  <div class="note">
    <p>Here <kbd>M</kbd> means meta or alt key, <kbd>C</kbd> means
    control key. </p>
  </div>
</jsp:root>